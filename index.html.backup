    const regulatoryRisk = parseFloat(document.getElementById('risk-regulatory').value) || 0;
    const competitiveRisk = parseFloat(document.getElementById('risk-competitive').value) || 0;
    
    const averageRisk = (marketRisk + financialRisk + operationalRisk + regulatoryRisk + competitiveRisk) / 5;
    
    const resultDiv = document.getElementById('risk-matrix-result');
    
    let riskLevel = 'Low Risk';
    let riskColor = 'var(--success-light)';
    
    if (averageRisk >= 7) {
        riskLevel = 'High Risk';
        riskColor = 'var(--error-light)';
    } else if (averageRisk >= 4) {
        riskLevel = 'Medium Risk';
        riskColor = 'var(--warning-light)';
    }
    
    resultDiv.style.background = riskColor;
    resultDiv.innerHTML = `<strong>Overall Risk Score:</strong><br>
        ${riskLevel} (${averageRisk.toFixed(1)}/10)<br>
        <small>Market: ${marketRisk} | Financial: ${financialRisk} | Operational: ${operationalRisk}</small>`;
}

