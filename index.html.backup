            });
            
            totalRevenue += revenue;
            totalProfit += netProfit;
            
            revenue *= (1 + growthRate);
            expenses *= (1 + inflationRate);
        }
        
        const avgROI = (totalProfit / (year1Revenue * 5)) * 100;
