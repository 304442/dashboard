function autoCalculateNPVFP() {
    const initialInvestment = parseFloat(document.getElementById('initial-investment-fp')?.value || 0);
    const discountRate = parseFloat(document.getElementById('discount-rate-fp')?.value || 0) / 100;
    const terminalValue = parseFloat(document.getElementById('terminal-value-fp')?.value || 0);
    const year1Revenue = parseFloat(document.getElementById('year1-revenue-fp')?.value || 0);
    const year1Expenses = parseFloat(document.getElementById('year1-expenses-fp')?.value || 0);
    const growthRate = parseFloat(document.getElementById('growth-rate-fp')?.value || 0) / 100;
    
    if (initialInvestment > 0 && discountRate > 0 && year1Revenue > 0) {
        let npv = -initialInvestment;
        let revenue = year1Revenue;
        let expenses = year1Expenses;
        let cumulativeCashFlow = -initialInvestment;
        let paybackPeriod = 0;
        
        for (let year = 1; year <= 5; year++) {
            const cashFlow = revenue - expenses;
            const discountedCashFlow = cashFlow / Math.pow(1 + discountRate, year);
            npv += discountedCashFlow;
            cumulativeCashFlow += cashFlow;
            
            if (paybackPeriod === 0 && cumulativeCashFlow > 0) {
                paybackPeriod = year - 1 + (Math.abs(cumulativeCashFlow - cashFlow) / cashFlow);
            }
            
            revenue *= (1 + growthRate);
            expenses *= 1.03;
        }
        
        npv += terminalValue / Math.pow(1 + discountRate, 5);
        const roi = ((npv + initialInvestment) / initialInvestment) * 100;
        const irr = calculateIRR(initialInvestment, year1Revenue, year1Expenses, growthRate, 5);
        
        const analysisText = `NPV: ${formatCurrency(npv)} | ROI: ${roi.toFixed(1)}% | IRR: ${irr.toFixed(1)}% | Payback: ${paybackPeriod.toFixed(1)} years`;
        document.getElementById('npv-text-fp').innerHTML = analysisText;
    }
}

function autoCalculateCashFlowFP() {
    const monthlyFixed = parseFloat(document.getElementById('monthly-fixed-costs')?.value || 0);
    const variablePct = parseFloat(document.getElementById('variable-cost-percentage')?.value || 0) / 100;
    const workingCapital = parseFloat(document.getElementById('working-capital')?.value || 0);
    const arDays = parseFloat(document.getElementById('ar-days')?.value || 0);
    const apDays = parseFloat(document.getElementById('ap-days')?.value || 0);
    const inventoryDays = parseFloat(document.getElementById('inventory-turnover')?.value || 0);
    const year1Revenue = parseFloat(document.getElementById('year1-revenue-fp')?.value || 0);
    
    if (monthlyFixed > 0 && year1Revenue > 0) {
        const monthlyRevenue = year1Revenue / 12;
        const monthlyVariableCosts = monthlyRevenue * variablePct;
        const monthlyCashFlow = monthlyRevenue - monthlyFixed - monthlyVariableCosts;
        const dso = arDays / 30;
        const dpo = apDays / 30;
        const daysInventory = inventoryDays / 30;
        
        const cashConversionCycle = dso + daysInventory - dpo;
        const workingCapitalImpact = (monthlyRevenue * cashConversionCycle) / 30;
        const netMonthlyCashFlow = monthlyCashFlow - workingCapitalImpact;
        
        const cashFlowText = `Monthly Operating Cash Flow: ${formatCurrency(monthlyCashFlow)}<br>
                             Cash Conversion Cycle: ${cashConversionCycle.toFixed(1)} days<br>
                             Working Capital Impact: ${formatCurrency(workingCapitalImpact)}<br>
                             Net Monthly Cash Flow: ${formatCurrency(netMonthlyCashFlow)}<br>
                             Annual Free Cash Flow: ${formatCurrency(netMonthlyCashFlow * 12)}`;
        
        document.getElementById('cash-flow-text-fp').innerHTML = cashFlowText;
    }
}

function autoCalculateBreakEvenFP() {
    const unitPrice = parseFloat(document.getElementById('unit-price-fp')?.value || 0);
    const unitVariableCost = parseFloat(document.getElementById('unit-variable-cost-fp')?.value || 0);
    const monthlyFixedCosts = parseFloat(document.getElementById('monthly-fixed-costs-be')?.value || 0);
    
    if (unitPrice > 0 && unitVariableCost < unitPrice && monthlyFixedCosts > 0) {
        const contributionMargin = unitPrice - unitVariableCost;
        const contributionMarginPct = (contributionMargin / unitPrice) * 100;
        const breakEvenUnits = monthlyFixedCosts / contributionMargin;
        const breakEvenRevenue = breakEvenUnits * unitPrice;
        const annualBreakEvenRevenue = breakEvenRevenue * 12;
        
        const safetyMargin = contributionMarginPct > 50 ? 'High' : contributionMarginPct > 30 ? 'Medium' : 'Low';
        
        const breakEvenText = `Break-Even Units (Monthly): ${Math.ceil(breakEvenUnits).toLocaleString()}<br>
                               Break-Even Revenue (Monthly): ${formatCurrency(breakEvenRevenue)}<br>
                               Break-Even Revenue (Annual): ${formatCurrency(annualBreakEvenRevenue)}<br>
                               Contribution Margin: ${formatCurrency(contributionMargin)} (${contributionMarginPct.toFixed(1)}%)<br>
                               Safety Margin: ${safetyMargin}`;
        
        document.getElementById('break-even-text-fp').innerHTML = breakEvenText;
    }
}

function calculateIRR(investment, revenue, expenses, growthRate, years) {
    let rate = 0.1;
    for (let i = 0; i < 100; i++) {
        let npv = -investment;
        let rev = revenue;
        let exp = expenses;
        for (let year = 1; year <= years; year++) {
            npv += (rev - exp) / Math.pow(1 + rate, year);
            rev *= (1 + growthRate);
            exp *= 1.03;
        }
        if (Math.abs(npv) < 0.01) break;
        rate += npv > 0 ? 0.01 : -0.01;
    }
    return rate * 100;
}

