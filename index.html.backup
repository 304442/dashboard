function autoCalculateProjections() {
    const year1Revenue = parseFloat(document.getElementById('year1-revenue').value) || 0;
    const year1Expenses = parseFloat(document.getElementById('year1-expenses').value) || 0;
    const growthRate = parseFloat(document.getElementById('growth-rate').value) || 0;
    const inflationRate = parseFloat(document.getElementById('inflation-rate').value) || 0;
    
    if (year1Revenue === 0) {
        document.getElementById('projections-text').textContent = 'Enter projections above to see 5-year forecast';
        return;
    }
    
    let totalProfit = 0;
    let projectionsText = '';
    
    for (let year = 1; year <= 5; year++) {
        const revenue = year1Revenue * Math.pow(1 + growthRate/100, year - 1);
        const expenses = year1Expenses * Math.pow(1 + inflationRate/100, year - 1);
        const profit = revenue - expenses;
        totalProfit += profit;
        
        projectionsText += `Year ${year}: $${profit.toLocaleString()} profit | `;
    }
    
    projectionsText += `Total 5-Year Profit: $${totalProfit.toLocaleString()}`;
    document.getElementById('projections-text').textContent = projectionsText;
}

function autoCalculateNPV() {
    const discountRate = parseFloat(document.getElementById('discount-rate').value) || 10;
    const terminalValue = parseFloat(document.getElementById('terminal-value').value) || 0;
    const taxRate = parseFloat(document.getElementById('tax-rate').value) || 25;
    const investment = parseFloat(document.getElementById('total-investment-req').value) || 450000;
    
    // Simple NPV calculation (assuming basic cash flows)
    const year1Revenue = parseFloat(document.getElementById('year1-revenue').value) || 180000;
    const year1Expenses = parseFloat(document.getElementById('year1-expenses').value) || 150000;
    const growthRate = parseFloat(document.getElementById('growth-rate').value) || 15;
    const inflationRate = parseFloat(document.getElementById('inflation-rate').value) || 3;
    
    let npv = -investment; // Initial investment
    let totalCashFlow = 0;
    
    for (let year = 1; year <= 5; year++) {
        const revenue = year1Revenue * Math.pow(1 + growthRate/100, year - 1);
        const expenses = year1Expenses * Math.pow(1 + inflationRate/100, year - 1);
        const pretaxProfit = revenue - expenses;
        const afterTaxCashFlow = pretaxProfit * (1 - taxRate/100);
        const discountedCashFlow = afterTaxCashFlow / Math.pow(1 + discountRate/100, year);
        npv += discountedCashFlow;
        totalCashFlow += afterTaxCashFlow;
    }
    
    // Add terminal value
    const discountedTerminalValue = terminalValue / Math.pow(1 + discountRate/100, 5);
    npv += discountedTerminalValue;
    
    const roi = ((totalCashFlow + terminalValue - investment) / investment) * 100;
    const paybackMonths = investment / (totalCashFlow / 60); // Rough payback calculation
    
    document.getElementById('npv-text').textContent = 
        `NPV: $${npv.toLocaleString()} | ROI: ${roi.toFixed(1)}% | Payback: ${paybackMonths.toFixed(0)} months`;
    
    // Update result color based on NPV
    const npvResult = document.getElementById('npv-result');
    if (npv > 0) {
        npvResult.style.background = 'var(--success-light)';
    } else {
