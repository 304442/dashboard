// Cash Flow Analysis Functions
function autoCalculateCashFlow() {
    const startingCash = parseFloat(document.getElementById('starting-cash').value) || 50000;
    const burnRate = parseFloat(document.getElementById('cash-burn-rate').value) || 8000;
    const rampupMonths = parseFloat(document.getElementById('revenue-rampup').value) || 6;
    
    // Simple cash flow calculation for first 12 months
    let cumulativeCash = startingCash;
    let breakEvenMonth = 0;
    
    for (let month = 1; month <= 12; month++) {
        const cashIn = month <= rampupMonths ? 
            (month * 4000) + 1000 : // Gradual ramp up
            25000 + (month - rampupMonths) * 2000; // Full operation
        const cashOut = burnRate + (month * 200); // Increasing expenses
        const netCashFlow = cashIn - cashOut;
        cumulativeCash += netCashFlow;
        
        if (netCashFlow > 0 && breakEvenMonth === 0) {
            breakEvenMonth = month;
        }
    }
    
    const runway = Math.floor(startingCash / burnRate);
    document.getElementById('cash-flow-text').textContent = 
        `Break-even month: ${breakEvenMonth || 'N/A'} | Runway: ${runway}+ months | Max cash needed: $${startingCash.toLocaleString()}`;
}

function generateCashFlow() {
    autoCalculateCashFlow();
    const tableBody = document.getElementById('cash-flow-tbody');
    const startingCash = parseFloat(document.getElementById('starting-cash').value) || 50000;
    const burnRate = parseFloat(document.getElementById('cash-burn-rate').value) || 8000;
    const rampupMonths = parseFloat(document.getElementById('revenue-rampup').value) || 6;
    
    let cumulativeCash = startingCash;
    let rows = '';
    
    for (let month = 1; month <= 12; month++) {
        const cashIn = month <= rampupMonths ? 
            (month * 3000) + 2000 : 
            25000 + (month - rampupMonths) * 2000;
        const cashOut = burnRate + (month * 150);
        const netCashFlow = cashIn - cashOut;
        cumulativeCash += netCashFlow;
        
        const status = netCashFlow < 0 ? 
            '<span class="badge badge-warning">Burning</span>' :
            cumulativeCash > startingCash * 1.5 ? 
                '<span class="badge badge-success">Strong</span>' :
                '<span class="badge badge-success">Positive</span>';
        
        rows += `<tr>
            <td>${month}</td>
            <td>$${cashIn.toLocaleString()}</td>
            <td>$${cashOut.toLocaleString()}</td>
            <td>${netCashFlow >= 0 ? '+' : ''}$${netCashFlow.toLocaleString()}</td>
            <td>$${cumulativeCash.toLocaleString()}</td>
            <td>${status}</td>
        </tr>`;
    }
    
    tableBody.innerHTML = rows;
}

